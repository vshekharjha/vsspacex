{
  "extends": "./.babelrc.json",
  "plugins": [
    [
      "file-loader",
      {
        "name": "[hash].[ext]",
        "extensions": [
          "png",
          "jpg",
          "jpeg",
          "gif",
          "svg"
        ],
        "publicPath": "/public/img",
        "outputPath": null
      },
      "img-file-loader-plugin"
    ],
    [
      "babel-plugin-transform-require-ignore",
      {
        "extensions": [
          ".css",
          ".sass",
          ".scss"
        ]
      }
    ]
  ]
}